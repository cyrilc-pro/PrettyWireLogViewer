{
  "name": "PrettyWireLogViewer",
  "version": "0.0.1",
  "authors": [
    "moznion <moznion@gmail.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "vue": "0.11.0-rc3",
    "lodash": "~2.4.1",
    "bootstrap": "~3.2.0",
    "utf8": "~2.0.0",
    "jquery": "~2.1.1",
    "micro-location.js": "cho45/micro-location.js#~0.1.4"
  },
  "overrides": {
    "vue": {
      "main": "**/*.min.js"
    },
    "lodash": {
      "main": "**/*.min.js"
    },
    "bootstrap": {
      "main": [
        "**/*.min.js",
        "**/*.min.css"
      ]
    },
    "utf8": {
      "main": "*.js"
    },
    "jquery": {
      "main": "**/jquery.min.js"
    },
    "micro-location.js": {
      "main": "lib/micro-location.js"
    }
  }
}
